pipeline {
agent any
stages {
  stage('git scm'){
  steps{
  	echo "this is a code fetching stage"
	}
	}
  stage('build'){
  steps{
  	echo "this is a stage to build"
	}
	}
  stage('test'){
  steps{
  	echo " this is a stage to test"
	}
	}
  stage('deploy'){
  steps{
  	echo "this is a stage to deploy"
	}
	}
}
}
  		
